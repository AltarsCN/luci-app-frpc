{
	"luci-app-frpc": {
		"description": "Grant access to LuCI app frpc",
		"read": {
			"file": {
				"/etc/passwd": ["read"],
				"/etc/group": ["read"]
			},
			"ubus": {
				"service": [ "list" ],
				"rc": [ "list" ]
			},
			"uci": ["frpc"]
		},
		"write": {
			"ubus": {
				"rc": [ "init" ]
			},
			"uci": ["frpc"]
		}
	}
}
