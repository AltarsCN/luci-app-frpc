{
	"luci-app-frpc": {
		"description": "Grant access to LuCI app frpc",
		"read": {
			"file": {
				"/etc/passwd": ["read"],
				"/etc/group": ["read"]
			},
			"ubus": {
				"service": [ "list" ],
				"luci": [ "getInitList" ]
			},
			"uci": ["frpc"]
		},
		"write": {
			"uci": ["frpc"],
			"ubus": {
				"luci": [ "setInitAction" ]
			}
		}
	}
}
